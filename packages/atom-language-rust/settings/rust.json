{
  ".source.rust": {
    "editor": {
      "autoIndentOnPaste": true,
      "commentStart": "//",
      "decreaseIndentPattern": "^\\s*[})\\]][;\\s]*$",
      "foldEndPattern": "^\\s*\\}",
      "increaseIndentPattern": "^.*(?:\\{[^}'\"]*|\\([^)'\"]*|\\[[^\\]'\"]*)$",
      "softTabs": true,
      "tabLength": 4
    }
  }
}
